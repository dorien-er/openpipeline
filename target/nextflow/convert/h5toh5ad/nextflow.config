  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    h5toh5ad__input = "viash_no_value"
    h5toh5ad__output = "output.h5ad"
    h5toh5ad__compression = "gzip"
