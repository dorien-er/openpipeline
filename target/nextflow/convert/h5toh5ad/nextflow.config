    testScript = "run_test.py"
    testResources = [ "run_test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5" ]
    h5toh5ad {
      name = "h5toh5ad"
      container = "convert_h5toh5ad"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "h5toh5ad"
      tests {
        isDefined = true
        testScript = "run_test.py"
        testResources = [ "run_test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5" ]
      }
      arguments {
        input {
          name = "input"
