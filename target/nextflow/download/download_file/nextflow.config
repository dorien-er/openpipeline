  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    download_file__input = "viash_no_value"
    download_file__output = "viash_no_value"
    download_file__verbose = "no_default_value_configured"
    id = ""
    testScript = "run_test.sh"
    testResources = [ "run_test.sh" ]
    download_file {
      name = "download_file"
      container = "download_download_file"
      containerTag = "main_build"
      containerRegistry = ""
      command = "download_file"
      tests {
        isDefined = true
        testScript = "run_test.sh"
        testResources = [ "run_test.sh" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_file__input}"
          example = "https://cf.10xgenomics.com/samples/cell-exp/3.0.0/pbmc_1k_protein_v3/pbmc_1k_protein_v3_raw_feature_bc_matrix.h5"
          description = "URL to a file to download."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_file__output}"
          example = "pbmc_1k_protein_v3_raw_feature_bc_matrix.h5"
          description = "Path where to store output."
        }
        verbose {
          name = "verbose"
          otype = "--"
          required = false
          type = "boolean_true"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_file__verbose}"
          description = "Increase verbosity"
        }
      }
    }
  }