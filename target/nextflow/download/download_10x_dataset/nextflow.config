  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    download_10x_dataset__input = "viash_no_value"
    download_10x_dataset__output = "viash_no_value"
    download_10x_dataset__min_library_size = "0"
    download_10x_dataset__min_cells_per_gene = "0"
    download_10x_dataset__keep_feature_types = "no_default_value_configured"
    id = ""
    testScript = "run_test.R"
    testResources = [ "run_test.R" ]
    download_10x_dataset {
      name = "download_10x_dataset"
      container = "download_download_10x_dataset"
      containerTag = "0.2.0"
      containerRegistry = "openpipeline"
      command = "download_10x_dataset"
      tests {
        isDefined = true
        testScript = "run_test.R"
        testResources = [ "run_test.R" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__input}"
          example = "https://cf.10xgenomics.com/samples/cell-exp/3.0.0/pbmc_1k_protein_v3/pbmc_1k_protein_v3_raw_feature_bc_matrix.h5"
          description = "Location URL to an h5 file from the 10x genomics website."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__output}"
          example = "pbmc_1k_protein_v3_raw_feature_bc_matrix.h5"
          description = "Output h5 file."
        }
        min_library_size {
          name = "min_library_size"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__min_library_size}"
          dflt = "0"
          description = "Minimum library size."
        }
        min_cells_per_gene {
          name = "min_cells_per_gene"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__min_cells_per_gene}"
          dflt = "0"
          description = "Minimum number of cells per gene."
        }
        keep_feature_types {
          name = "keep_feature_types"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.download_10x_dataset__keep_feature_types}"
          example = "Antibody Capture"
          description = "Specify which feature types will never be filtered out"
        }
      }
    }
  }