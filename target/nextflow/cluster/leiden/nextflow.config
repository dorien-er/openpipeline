  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    leiden__input = "viash_no_value"
    leiden__modality = "rna"
    leiden__output = "viash_no_value"
    leiden__obs_name = "leiden"
    leiden__resolution = "1.0"
    id = ""
    testScript = "run_test.py"
    testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
    leiden {
      name = "leiden"
      container = "cluster_leiden"
      containerTag = "0.3.0rc2"
      containerRegistry = "ghcr.io"
      containerOrganization = "openpipelines-bio"
      command = "leiden"
      tests {
        isDefined = true
        testScript = "run_test.py"
        testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__input}"
          example = "input.h5mu"
          description = "Input file."
        }
        modality {
          name = "modality"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.leiden__modality}"
          dflt = "rna"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__output}"
          example = "output.h5mu"
          description = "Output file."
        }
        obs_name {
          name = "obs_name"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__obs_name}"
          dflt = "leiden"
          description = "Name of the .obs key under which to add the cluster labels."
        }
        resolution {
          name = "resolution"
          otype = "--"
          required = false
          type = "double"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__resolution}"
          dflt = "1.0"
          description = "A parameter value controlling the coarseness of the clustering. Higher values lead to more clusters."
        }
      }
    }
  }