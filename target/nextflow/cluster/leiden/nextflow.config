  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    leiden__input = "viash_no_value"
    leiden__output = "viash_no_value"
    leiden__outputFormat = "h5ad"
    leiden__clusterColumnName = "leiden.res.0.25"
    leiden__resolution = "0.25"
    id = ""
    testScript = "run_test.py"
    testResources = [ "run_test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.norm.hvg.pca.nn.umap.h5ad" ]
    leiden {
      name = "leiden"
      container = "cluster_leiden"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "leiden"
      tests {
        isDefined = true
        testScript = "run_test.py"
        testResources = [ "run_test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.norm.hvg.pca.nn.umap.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__input}"
          description = "Input h5ad file"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__output}"
          description = "Output file."
        }
        outputFormat {
          name = "outputFormat"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__outputFormat}"
          dflt = "h5ad"
          description = "The output format for the clustering. This can be csv or h5ad."
        }
        clusterColumnName {
          name = "clusterColumnName"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__clusterColumnName}"
          dflt = "leiden.res.0.25"
          description = "The column name for the clustering."
        }
        resolution {
          name = "resolution"
          otype = "--"
          required = false
          type = "double"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__resolution}"
          dflt = "0.25"
          description = "The resolution to run the clustering in."
        }
      }
    }
  }