  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    leiden__input = "viash_no_value"
    leiden__output = "viash_no_value"
    leiden__output_key = "neighbors"
    leiden__resolution = "1.0"
    leiden__cluster_column_name = "leiden.res.0.25"
    id = ""
    testScript = "run_test.py"
    testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
    leiden {
      name = "leiden"
      container = "cluster_leiden"
      containerTag = "0.3.0rc1"
      containerRegistry = "ghcr.io"
      containerOrganization = "openpipelines-bio"
      command = "leiden"
      tests {
        isDefined = true
        testScript = "run_test.py"
        testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__input}"
          example = "input.h5mu"
          description = "Input file."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__output}"
          example = "output.h5mu"
          description = "Output file."
        }
        output_key {
          name = "output_key"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__output_key}"
          dflt = "neighbors"
          description = "The .obs key under which to add the cluster labels."
        }
        resolution {
          name = "resolution"
          otype = "--"
          required = false
          type = "double"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__resolution}"
          dflt = "1.0"
          description = "A parameter value controlling the coarseness of the clustering. Higher values lead to more clusters."
        }
        cluster_column_name {
          name = "cluster_column_name"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.leiden__cluster_column_name}"
          dflt = "leiden.res.0.25"
          description = "Name of the adata.obs key under which to add the cluster labels."
        }
      }
    }
  }