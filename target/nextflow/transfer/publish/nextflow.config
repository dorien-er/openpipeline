  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    publish__input = "viash_no_value"
    publish__output = "viash_no_value"
    id = ""
    testScript = "run_test.sh"
    testResources = [ "run_test.sh" ]
    publish {
      name = "publish"
      container = "transfer_publish"
      containerTag = "0.3.0rc2"
      containerRegistry = "ghcr.io"
      containerOrganization = "openpipelines-bio"
      command = "publish"
      tests {
        isDefined = true
        testScript = "run_test.sh"
        testResources = [ "run_test.sh" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.publish__input}"
          description = "Input filename"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.publish__output}"
          description = "Output filename"
        }
      }
    }
  }