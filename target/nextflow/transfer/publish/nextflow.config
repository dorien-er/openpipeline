  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    publish__input = "${params.input}"
    publish__output = "${params.output}"
    id = ""
    input = ""
    output = ""
    testScript = "test.sh"
    testResources = [ "test/test.sh" ]
    publish {
      name = "publish"
      container = "transfer_publish"
      containerTag = "dev"
      containerRegistry = ""
      command = "publish"
      tests {
        isDefined = true
        testScript = "test.sh"
        testResources = [ "test/test.sh" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.publish__input}"
          description = "Input filename"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.publish__output}"
          description = "Output filename"
        }
      }
    }
  }