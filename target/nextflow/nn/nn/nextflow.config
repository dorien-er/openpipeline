  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    nn__input = "viash_no_value"
    nn__output = "no_default_value_configured"
    nn__metric = "cosine"
    nn__nNeighbors = "15"
    id = ""
    testScript = "run_test.sh"
    testResources = [ "run_test.sh", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
    nn {
      name = "nn"
      container = "nn_nn"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "nn"
      tests {
        isDefined = true
        testScript = "run_test.sh"
        testResources = [ "run_test.sh", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.nn__input}"
          description = "Input h5ad file"
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.nn__output}"
          description = "Output h5ad file containing the dimred object."
        }
        metric {
          name = "metric"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.nn__metric}"
          dflt = "cosine"
          description = "The distance metric to be used in the generation of the nearest neighborhood network."
        }
        nNeighbors {
          name = "nNeighbors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.nn__nNeighbors}"
          dflt = "15"
          description = "The number of neighors to be present in the nn-network."
        }
      }
    }
  }