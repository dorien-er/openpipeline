  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    hvg_scanpy__input = "viash_no_value"
    hvg_scanpy__output = "no_default_value_configured"
    hvg_scanpy__excludedGenes = ""
    hvg_scanpy__flavor = "seurat"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
    hvg_scanpy {
      name = "hvg_scanpy"
      container = "hvg_hvg_scanpy"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "hvg_scanpy"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__input}"
          description = "Input h5ad file"
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__output}"
          description = "Write the parameters to a json file."
        }
        excludedGenes {
          name = "excludedGenes"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__excludedGenes}"
          dflt = ""
          description = "Genes to exclude from the highly variable genes. This can be of use when performing for example analysis on B/T cells which have variable genes that should not be taken into account."
        }
        flavor {
          name = "flavor"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__flavor}"
          dflt = "seurat"
          description = "The flavor to select for highly variable genes. Default seurat, other options seurat_v3 or cell_ranger"
        }
      }
    }
  }