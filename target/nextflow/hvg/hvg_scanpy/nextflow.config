  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    hvg_scanpy__input = "viash_no_value"
    hvg_scanpy__output = "no_default_value_configured"
    hvg_scanpy__excluded_genes = ""
    hvg_scanpy__flavor = "seurat"
    id = ""
    testScript = "run_test.py"
    testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
    hvg_scanpy {
      name = "hvg_scanpy"
      container = "hvg_hvg_scanpy"
      containerTag = "main_build"
      containerRegistry = "ghcr.io"
      command = "hvg_scanpy"
      tests {
        isDefined = true
        testScript = "run_test.py"
        testResources = [ "run_test.py", "../../../resources_test/pbmc_1k_protein_v3" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__input}"
          example = "input.h5mu"
          description = "Input h5mu file"
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__output}"
          example = "output.h5mu"
          description = "Output h5mu file"
        }
        excluded_genes {
          name = "excluded_genes"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__excluded_genes}"
          dflt = ""
          description = "Genes to exclude from the highly variable genes. This can be of use when performing for example analysis on B/T cells which have variable genes that should not be taken into account."
        }
        flavor {
          name = "flavor"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.hvg_scanpy__flavor}"
          dflt = "seurat"
          description = "The flavor to select for highly variable genes. Default seurat, other options seurat_v3 or cell_ranger"
        }
      }
    }
  }